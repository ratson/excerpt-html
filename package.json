  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "he": "^1.2.0",
    "lodash.truncate": "^4.4.2",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "tap": "^12.1.1"
  },
  "repository": "ratson/excerpt-html",
  "publishConfig": {
    "access": "public"
  }
}
